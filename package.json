{
  "name": "react-frontend-boilerplate",
  "version": "0.0.1",
  "description": "A simple frontend only boilerplate for quick/easy react learning/experimenting. Made with Innovative Design Web Tier in mind.",
  "scripts": {
    "build": "webpack -p --env=prod --progress --profile --colors",
    "clean": "rm -rf ./dist",
    "dev": "NODE_ENV=dev webpack-dev-server",
    "lint": "eslint src",
    "postinstall": "cp node_modules/normalize.css/normalize.css styles/vendor",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cle1994/react-frontend-boilerplate"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "babel",
    "boilerplate",
    "webpack"
  ],
  "author": "Christian Le <hi@christianle.com> (https://github.com/cle1994)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cle1994/react-frontend-boilerplate/issues"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "jest": "^18.1.0",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.4.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.3.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true
  }
}
